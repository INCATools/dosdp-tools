Prefix(:=<owlapi:ontology#ont1#>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)


Ontology(<owlapi:ontology#ont1>

Declaration(Class(<http://example.org/exposure/1>))
Declaration(Class(<http://example.org/exposure/2>))
Declaration(Class(<http://example.org/exposure/3>))
Declaration(Class(<http://purl.obolibrary.org/obo/CHEBI_46661>))
Declaration(Class(<http://purl.obolibrary.org/obo/CHEBI_59999>))
Declaration(Class(<http://purl.obolibrary.org/obo/CHEBI_75701>))
Declaration(Class(<http://purl.obolibrary.org/obo/ExO_0000002>))
Declaration(ObjectProperty(<http://purl.obolibrary.org/obo/RO_0002233>))
Declaration(AnnotationProperty(<http://purl.obolibrary.org/obo/IAO_0000115>))



############################
#   Classes
############################

# Class: <http://example.org/exposure/1> (exposure to sarin)

AnnotationAssertion(<http://purl.obolibrary.org/obo/IAO_0000115> <http://example.org/exposure/1> "A exposure event involving the interaction of an exposure receptor to sarin. Exposure may be through a variety of means, including through the air or surrounding medium, or through ingestion."^^xsd:string)
AnnotationAssertion(rdfs:label <http://example.org/exposure/1> "exposure to sarin"^^xsd:string)
EquivalentClasses(<http://example.org/exposure/1> ObjectIntersectionOf(<http://purl.obolibrary.org/obo/ExO_0000002> ObjectSomeValuesFrom(<http://purl.obolibrary.org/obo/RO_0002233> <http://purl.obolibrary.org/obo/CHEBI_75701>)))

# Class: <http://example.org/exposure/2> (exposure to asbestos)

AnnotationAssertion(<http://purl.obolibrary.org/obo/IAO_0000115> <http://example.org/exposure/2> "A exposure event involving the interaction of an exposure receptor to asbestos. Exposure may be through a variety of means, including through the air or surrounding medium, or through ingestion."^^xsd:string)
AnnotationAssertion(rdfs:label <http://example.org/exposure/2> "exposure to asbestos"^^xsd:string)
EquivalentClasses(<http://example.org/exposure/2> ObjectIntersectionOf(<http://purl.obolibrary.org/obo/ExO_0000002> ObjectSomeValuesFrom(<http://purl.obolibrary.org/obo/RO_0002233> <http://purl.obolibrary.org/obo/CHEBI_46661>)))

# Class: <http://example.org/exposure/3> (exposure to chemical substance)

AnnotationAssertion(<http://purl.obolibrary.org/obo/IAO_0000115> <http://example.org/exposure/3> "A exposure event involving the interaction of an exposure receptor to chemical substance. Exposure may be through a variety of means, including through the air or surrounding medium, or through ingestion."^^xsd:string)
AnnotationAssertion(rdfs:label <http://example.org/exposure/3> "exposure to chemical substance"^^xsd:string)
EquivalentClasses(<http://example.org/exposure/3> ObjectIntersectionOf(<http://purl.obolibrary.org/obo/ExO_0000002> ObjectSomeValuesFrom(<http://purl.obolibrary.org/obo/RO_0002233> <http://purl.obolibrary.org/obo/CHEBI_59999>)))


